<%-include("header")%>
<main class="todocorner-main">
    <div class="todocorner-login">
        <h2>Sign Up</h2>
        <form action="/signup" method="post">
            <input type="text" class="todocorner-input" placeholder="username......." name="name">
            <input type="text" class="todocorner-input" placeholder="email......." name="email">
            <input type="tel" class="todocorner-input" placeholder="phone no....." name="phone">
            <input type="password" class="todocorner-input" placeholder="password......." name="password">
            <button type="submit" class="todocorner-button">Register</button>
        </form>
        <hr class="signin-line">
        <p class="todocorner-signup">Already have account? <a class="logina" href="/">Login</a></p>
    </div>
</main>
<%-include("footer")%>